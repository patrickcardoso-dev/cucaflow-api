generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "rhel-openssl-1.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id            String  @id @default(uuid())
  username      String
  email         String  @unique
  password      String
  avatar        String?
  isLoginSocial Boolean @default(false)
}
